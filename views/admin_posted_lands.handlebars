<div class="container-fluid">
    <div>
        <h3 class="lp-header">Posted Lands</h3>
    </div>
    <div class="container">
        <div class="row">
            <div class="col-lg-6 col-md-12 col-sm-12 mt-3 search-div">
                <div class="form">
                    <i class="fa fa-search"></i>
                    <input type="text" class="form-control form-input" placeholder="Search anything...">
                    <span class="left-pan"><i class="fa fa-microphone"></i></span>
                </div>
            </div>
        </div>
    </div>
    <div class="row">
        <div class="col-lg-12 col-md-12 col-sm-12 land-item">
            <table class="table table-striped">
                <thead>
                    <tr>
                        <th scope="col">#</th>
                        <th scope="col">Title deed no.</th>
                        <th scope="col">Land size</th>
                        <th scope="col">Location</th>
                        <th scope="col">verification status</th>
                        <th scope="col"></th>
                    </tr>
                </thead>
                <tbody>
                    {{#each all_land_list}}
                    <tr>
                        <th scope="row">1</th>
                        <td>{{title_num}}</td>
                        <td>Otto</td>
                        <td>@mdo</td>
                        <td>@mdo</td>
                        <td><i class="fa fa-trash-alt"></i></td>
                    </tr>
                    {{/each}}
                </tbody>
            </table>
        </div>
        <div class="col-lg-12 col-md-12 col-sm-12 land-item">

        </div>
    </div>
</div>